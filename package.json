{
  "name": "advanced-node",
  "version": "1.0.0",
  "description": "Advanced node.js course with TDD, clean architecture and consumption of third-party apis.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/api",
    "build": " rimraf  dist && tsc -p tsconfig-build.json",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-chache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trajanovinicius/advanced-node.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/trajanovinicius/advanced-node/issues"
  },
  "homepage": "https://github.com/trajanovinicius/advanced-node#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.5",
    "lint-staged": "^12.3.7",
    "ts-jest": "^27.1.4",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "module-alias": "^2.2.2",
    "rimraf": "^3.0.2"
  }
}
